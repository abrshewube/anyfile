{
  "name": "@anyfile/excel",
  "version": "0.3.0",
  "description": "Excel handler for the AnyFile ecosystem.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.declarations.json --force && tsup src/index.ts --format cjs,esm --tsconfig tsconfig.json",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "anyfile",
    "excel",
    "xlsx",
    "spreadsheet",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@anyfile/core": "^0.1.1"
  },
  "dependencies": {
    "@anyfile/core": "^0.1.1",
    "xlsx": "^0.18.5",
    "xlsx-calc": "^0.9.2",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  }
}

